    "@babel/preset-typescript": "^7.27.1",
    "@eslint/js": "^9.38.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/jest": "^30.0.0",
    "babel-jest": "^30.2.0",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
