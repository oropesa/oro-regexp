    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-unicorn": "^61.0.2",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
